import java.util.Scanner;

class number  
{
	public static void main(String[] args) 
	{
		int num;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter A Number");
		num = sc.nextInt();
		if(num%2 == 0)
			System.out.println(num+" Number is Even");
		else
			System.out.println(num+" Number is odd");
		}
}
